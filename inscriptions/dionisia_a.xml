                    <lb n="2"/><num value="9">VIIII</num> <expan><abbr><unclear>m</unclear></abbr><ex>ensibus</ex></expan> <num value="7">VII</num> <expan><abbr>d</abbr><ex>iebus</ex></expan> <gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost" evidence="previouseditor" corresp="#bertolini1874a #bertolini1874b">cum</supplied>
