            <div type="apparatus">
                <app loc="3" from="#w1">
                    <lem>
                        amici
                    </lem>
                    <rdg source="#bertolini1875b">
                        amici<supplied reason="omitted">s</supplied>
                    </rdg>
                </app>
                <app loc="3" from="#w2">
                    <lem>
                        <choice><reg cert="low">honore</reg><orig>hore</orig></choice>
                    </lem>
                    <rdg source="#cilv8771">
                        <choice><reg cert="low">ex cura</reg><orig>hore</orig></choice>
                    </rdg>
                    <rdg source="#ils1962">
                        <choice><reg cert="low">ex opera</reg><orig>hore</orig></choice>
                    </rdg>
                </app>
